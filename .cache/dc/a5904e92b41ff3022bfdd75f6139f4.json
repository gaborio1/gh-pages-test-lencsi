 width: 100%;\n  }\n  \n  /* Add some top padding to the page content to prevent sudden quick movement (as the navigation bar gets a new position at the top of the page (position:fixed and top:0) */\n  .sticky + .content {\n    padding-top: 60px;\n  }\n\n  \nh1 {\n    color: #26a4ca;\n    color: #000000;\n    text-align: center;\n    font-size: 75px;\n}\n\nh2 {\n\tfont-size: 45px;\n    color: #4a5ab3;\n    text-align: center;\n}\n\np {\n    margin: 1.2rem;\n\tfont-size: 30px;\n}\n\n\n\nimg {\n    width: 600px;\n    max-width: 100%;\n    height: auto;\n    object-fit: contain;\n    padding: 5px;\n    border-radius: 1rem;\n}\n\n.row {\n    display: flex;\n    margin-bottom: 10px;\n}\n\n.column {\n    /* border: 2px solid #9298ba; */\n    width: 50%;\n    height: fit-content\n}\n\n\n.main {\n    position: relative;\n    display: flex;\n    // flex-direction: column;\n    align-items: center;\n    justify-content: center;\n    \n\theight: calc(100vh - 150px);\n    margin: 0px auto;\n    // margin-top: 50px;\n    // margin-bottom: 50px;\n\n\t// !!!\n    // max-width: 1200px;\n\n    background: #fee1e1;\n    background: #fee1e183;\n    background: beige; \n    background: transparent; \n\t// border: 5px solid green;\n}\n\n// .home, .drawings, .about (FULL WIDTH)\n.full-width-drw {\n\tposition: absolute;\n\tleft: 0;\n\tdisplay: flex;\n\tjustify-content: center;\n\twidth: 100%;\n\n\t// THIS WILL ALIGN CONTENT TO LEFT:\n\t// max-width: 1200px;\n\n\theight: 90%;\n\t// height: calc(100vh - 200px);\n\n\t// HIDE OVERFOLOW TO ACCOMODATE HEIGHT ANIMATION\n\toverflow: hidden;\n\t// border: 1px solid rebeccapurple;\n}\n\n// .home, .drawings {\n// \tbackground-image: linear-gradient(to right, #ffecd2 0%, #fcb69f 100%);\n// }\n\n// .home-content, .drawings-content, .about-content (MAX WIDTH: 1200)\n.max-width-content {\n\t// position: absolute;\n\twidth: 100%;\n\tmax-width: 1200px;\n\tborder-radius: 1em;\n\t// border: 5px solid rgb(19, 137, 139);\n}\n\n.max-width-content.non-about {\n\t// background-image: linear-gradient(to right, #ffecd2 0%, #fcb69f 100%);\n\t// background-image: linear-gradient(to right, #ffecd28a 0%, #fcb69fb0 100%);\n\tbackground-color: rgba(255, 123, 0, 0.226);\n}\n\n.about {\n\t// max-width: 1200px;\n\toverflow: scroll;\n\t// background-image: linear-gradient(to right, #ffecd2 0%, #fcb69f 100%);\n}\n\n// ===== HOME =====\n\n.max-width-content.home-content {\n\tbackground-image: url(\"../images/butterfly.jpg\");\n\tbackground-size: cover;\n\t// !!! ONLY WORKS ON PAGE LOAD !!!\n\t// opacity: 0.3;\n\n\th1 {\n\t\tmargin-top: 5%;\n\t}\n}\n\nfooter {\n    // background-color: #f8b1d8;\n\tbackground-color: #f93aa3c6;\n    width: 100%;\n    height: 100px;\n}\n\n\n.gallery-container {\n\twidth: 95%;\n\tmargin: 20px auto;\n\t// border: 3px solid royalblue;\n\n\timg.modal {\n\t\twidth: 100px;\n\t\twidth: 12%;\n\t\t// border: 3px solid red;\n\t}\n}\n\n"},"lineCount":null}},"error":null,"hash":"e26440e219255685bcea47b647e75361","cacheData":{"env":{}}}